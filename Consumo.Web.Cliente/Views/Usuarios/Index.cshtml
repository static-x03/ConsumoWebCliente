@model IEnumerable<Consumo.Web.Cliente.Models.Usuarios>

@{
    ViewBag.Title = "Index";
}

<h2>Consumo Web Api</h2>

<p>
    @Html.ActionLink("Nuevo Usuario","Create")
</p>
<div class="form-horizontal">
    <div class="form-group">
        x   
        <table class="table table-responsive" style="width:auto">
            <th>ID</th>
            <th>Status</th>
            <th>Fecha</th>
            <th>Nombre</th>
            <th>Dirección</th>
            <th>Departamento</th>
            <th>Teléfono</th>
            <th></th>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.int_id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.str_status)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.dat_fecha)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.str_nombre)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.str_direccion)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.str_departamento)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.int_telefono)
                    </td>
                    <td>
                        @Html.ActionLink("Editar", "Edit", new { id = item.int_id }) |
                        @Html.ActionLink("Eliminar", "Delete", new {id=item.int_id })
                    </td>

                </tr>
            }
        </table>

    </div>
</div>

